{
    "name": "devcake-deventer/laravel-loki-logging",
    "version": "0.0.1",
    "description": "Logging to Loki for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Sven Mol",
            "email": "s.mol@devcake.nl"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "illuminate/support": "^8.0.0",
        "illuminate/console": "^8.0.0",
        "monolog/monolog": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Devcake\\LaravelLokiLogging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devcake\\LaravelLokiLogging\\L3ServiceProvider"
            ]
        }
    }
}
